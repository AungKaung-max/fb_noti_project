{
    "version": 2,
    "builds": [
      {
        "src": "server/src/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "facebook/package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "facebook/build" }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server/src/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "facebook/build/$1"
      }
    ]
  }
  